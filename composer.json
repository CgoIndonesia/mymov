{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ANDRU19/yii2-dynamicform"
        },
         {
            "type": "vcs",
            "url": "https://github.com/ANDRU19/yii2-widget-fileinput"
        }
    ],

    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "almasaeed2010/adminlte" : "2.*",
        "fortawesome/font-awesome" : "*",
        "mdmsoft/yii2-admin": "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "kartik-v/yii2-widget-switchinput": "*",
        "fedemotta/yii2-widget-datatables": "*",
        "wbraganca/yii2-nested-set-behavior": "dev-master",
        "andru19/yii2-fancytree-widget": "dev-master",
        "faryshta/yii2-jquery-tagsinput": "dev-master",
        "vova07/yii2-imperavi-widget": "*",
        "bajadev/yii2-ckeditor" : "*",
        "kartik-v/yii2-widget-fileinput": "dev-downgrade",
        "kartik-v/yii2-checkbox-x": "dev-master",
        "yiisoft/yii2-imagine": "~2.0.0",
        "kartik-v/yii2-sortable-input": "dev-master",
        "wbraganca/yii2-dynamicform": "dev-bugfix"


    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
